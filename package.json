{
  "name": "weather-to",
  "version": "0.0.0",
  "description": "Weather activity planning.",
  "main": "index.js",
  "scripts": {
    "start": "serve --port $PORT build",
    "serve": "serve public",
    "postinstall": "bower install && grunt -v postinstall",
    "test": "npm run test:lint && npm run test:unit:phantom",
    "test:lint": "npm run test:lint:src && npm run test:lint:test",
    "test:lint:src": "eslint public/js/**/*.js",
    "test:lint:test": "eslint -c .eslintrc.test.js protractor.conf.js karma.conf.js test/**/*.js",
    "test:unit:phantom": "karma start --single-run --browsers PhantomJS",
    "test:unit:browser": "karma start --single-run",
    "test:e2e": "protractor protractor.conf.js"
  },
  "author": "Anders D. Johnson",
  "license": "UNLICENSED",
  "dependencies": {},
  "devDependencies": {
    "bower": "^1.3.12",
    "eslint": "^3.10.2",
    "eslint-plugin-jasmine": "^2.1.0",
    "grunt": "^0.4.5",
    "grunt-bower-requirejs": "^2.0.0",
    "grunt-cli": "^0.1.13",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-contrib-concat": "^0.5.0",
    "grunt-contrib-connect": "^0.9.0",
    "grunt-contrib-copy": "^0.7.0",
    "grunt-contrib-cssmin": "^0.11.0",
    "grunt-contrib-htmlmin": "^0.3.0",
    "grunt-contrib-less": "^0.12.0",
    "grunt-contrib-requirejs": "^0.4.4",
    "grunt-contrib-uglify": "^0.7.0",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-ng-annotate": "^3.0.0",
    "grunt-template": "^0.2.3",
    "jasmine-core": "^2.5.2",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-requirejs": "^1.1.0",
    "load-grunt-tasks": "^1.0.0",
    "protractor": "^4.0.11",
    "requirejs": "^2.3.2",
    "serve": "^1.4.0",
    "webdriver-manager": "^10.2.8"
  }
}
